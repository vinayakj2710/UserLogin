});\r\n  };\r\n\r\n  const validatePasswordHandler = () => {\r\n    // setPasswordIsValid(enteredPassword.trim().length > 6);\r\n    dispatchPass({type: 'INPUT_BLUR'});\r\n  };\r\n\r\n  const submitHandler = (event) => {\r\n    event.preventDefault();\r\n    authCtx.onLogin(emailState.value, passState.value);\r\n  };\r\n\r\n  return (\r\n    <Card className={classes.login}>\r\n      <form onSubmit={submitHandler}>\r\n        <Input\r\n          isValid={emailIsValid}\r\n          id=\"email\"\r\n          label=\"Email\"\r\n          type=\"email\"\r\n          value={emailState.value}\r\n          onChange={emailChangeHandler}\r\n          onBlur={validateEmailHandler}\r\n        />\r\n        <Input\r\n          isValid={passwordIsValid}\r\n          id=\"pass\"\r\n          label=\"Password\"\r\n          type=\"password\"\r\n          value={passState.value}\r\n          onChange={passwordChangeHandler}\r\n          onBlur={validatePasswordHandler}\r\n        />\r\n        <div className={classes.actions}>\r\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\r\n            Login\r\n          </Button>\r\n        </div>\r\n      </form>\r\n    </Card>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\Home\\Home.js",[],"C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\MainHeader\\Navigation.js",[],"C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\UI\\Button\\Button.js",[],"C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\UI\\Card\\Card.js",[],"C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\Store\\Auth-context.js",[],"C:\\Users\\Vinayak Jadhav\\OneDrive\\Desktop\\web-dev\\React\\UserLogin\\src\\components\\UI\\Input\\Input.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","rep